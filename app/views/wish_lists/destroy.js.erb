$("#wish-list-<%=@wish_list[:product_id]%>").html("<%= j (render partial: 'wish_lists/form', :locals => {:product => @product}) %>");
$("#add-product-to-wish-list").html("<%= j (render partial: 'wish_lists/form', :locals => {:product => @product}) %>");
